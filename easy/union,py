def main():
    arr=[2,3,1,4,32,6]
    arr2=[5,7,1,2,5,7]
    b=[]
    b=union(arr,arr2) 
    print(b)  
def union(arr,arr2):
    s=set()
    un=[]
    for i in arr:
        s.add(i) 
    for k in arr2:
        s.add(k)
    for num in s:
        un.append(num) 
    return un 
main()

      
        
                